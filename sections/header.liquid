                {% for link in section.settings.menu.links %}
                  <li class="nav-item bb-dropdown flex items-center relative mr-[2.8125rem] group">
                    {% if link.links != blank %}
                      <div class="relative group">
                        <a
                          href="{{ link.url }}"
                          class="flex items-center gap-x-1 text-[.9375rem] font-medium text-[#3d4750] hover:text-[#6c7fd8] transition-all duration-200"
                        >
                          {{ link.title }}
